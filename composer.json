{
    "name": "sonata-project/twig-extensions",
    "type": "symfony-bundle",
    "description": "Symfony twig extensions",
    "keywords": [
        "twig"
    ],
    "homepage": "https://sonata-project.org/bundles/twig-extensions",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Rabaix",
            "email": "thomas.rabaix@sonata-project.org",
            "homepage": "https://sonata-project.org"
        },
        {
            "name": "Sonata Community",
            "homepage": "https://github.com/sonata-project/twig-extensions/contributors"
        }
    ],
    "require": {
        "php": "^7.1",
        "sonata-project/doctrine-extensions": "^1.0",
        "symfony/http-foundation": "^3.4 || ^4.0",
        "symfony/translation": "^3.4 || ^4.0",
        "twig/twig": "^1.34 || ^2.0"
    },
    "require-dev": {
        "symfony/config": "^3.4 || ^4.0",
        "symfony/dependency-injection": "^3.4 || ^4.0",
        "symfony/http-kernel": "^3.4 || ^4.0",
        "symfony/phpunit-bridge": "^3.3 || ^4.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Sonata\\TwigExtensions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sonata\\TwigExtensions\\Tests\\": "tests/"
        }
    }
}
